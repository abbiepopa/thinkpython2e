    "def sumall(*args):\n",
    "    out = args[0]\n",
    "    i = 0\n",
    "    while i < len(args) - 1:\n",
    "        i += 1\n",
    "        out += args[i]\n",
    "    return(out)\n",
    "sumall(1,2,3,5)"
   ]
